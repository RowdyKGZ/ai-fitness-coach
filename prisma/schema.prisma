generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Assistant {
  id          String @id @default(uuid())
  assistantId String @unique
}

model User {
  id        String @id @default(uuid())
  userId    String @unique
  threadId  String
  createdAt DateTime @default(now())
}